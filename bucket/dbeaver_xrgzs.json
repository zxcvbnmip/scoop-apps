{
    "version": "25.3.0",
    "description": "Free universal database tool and SQL client",
    "homepage": "https://dbeaver.io",
    "license": "Apache-2.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/dbeaver/dbeaver/releases/download/25.3.0/dbeaver-ce-25.3.0-win32.win32.x86_64.zip",
            "hash": "a56e4f5cde5c3316824b005ab04187e226585b119bce061dff93f12d9dde50e6"
        }
    },
    "extract_dir": "dbeaver",
    "bin": [
        "dbeaver.exe",
        "dbeaverc.exe"
    ],
    "shortcuts": [
        [
            "dbeaver.exe",
            "DBeaver"
        ]
    ],
    "installer": {
        "script": [
            "Remove-Item \"$dir\\`$*\" -Recurse -Force",
            ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
            "New-PersistDirectory \"$env:APPDATA\\DBeaverData\" \"$persist_dir\\AppData\" -Migrate"
        ]
    },
    "pre_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "Stop-App",
        "Remove-Junction \"$env:APPDATA\\DBeaverData\""
    ],
    "checkver": {
        "github": "https://github.com/dbeaver/dbeaver"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/dbeaver/dbeaver/releases/download/$version/dbeaver-ce-$version-win32.win32.x86_64.zip"
            }
        }
    }
}
