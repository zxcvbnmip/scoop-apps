{
    "$schema": "https://raw.githubusercontent.com/ScoopInstaller/Scoop/master/schema.json",
    "homepage": "https://github.com/kiennq/emacs-build",
    "license": "GPLv3",
    "version": "31.330.20250201.668d096",
    "url": "https://github.com/kiennq/emacs-build/releases/download/v31.330.20250201.668d096/emacs-668d096-ucrt-x86_64.msix",
    "hash": "53d7c4d85d3233fe522277f99255a7fc228e074ab10e72610cb9b3ac940b4cea",
    "installer": {
        "script": [
            "Get-ChildItem \"$dir\\*.msix\" | %{Expand-7zipArchive $_.FullName \"$dir\"}",
            "Remove-Item -Force \"$dir\\*.msix\"",
            "Push-Location \"$dir\"",
            "dir 'bin\\*%2b*' | %{ren \"$_\" \"$($_.Name -replace '%2b','+')\"}",
            "Pop-Location"
        ]
    },
    "bin": [
        "bin\\runemacs.exe",
        "bin\\emacs.exe",
        "bin\\emacsclient.exe",
        "bin\\emacsclientw.exe",
        [
            "bin\\emacsclientw.exe",
            "emw",
            "-c -n -a \"\""
        ]
    ],
    "checkver": {
        "github": "https://github.com/kiennq/emacs-build",
        "regex": "/tag/v([\\d\\w.-]+)"
    },
    "autoupdate": {
        "url": "https://github.com/kiennq/emacs-build/releases/download/v$version/emacs-$buildVersion-ucrt-x86_64.msix"
    },
    "shortcuts": [
        [
            "bin\\runemacs.exe",
            "Emacs"
        ],
        [
            "bin\\emacsclientw.exe",
            "Emacs Client",
            "-c -n -a \"\""
        ]
    ]
}
